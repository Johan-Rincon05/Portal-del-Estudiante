  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development DATABASE_URL=postgresql://postgres:postgres@localhost:5432/portal_estudiante tsx server/index.ts",
    "dev:full": "concurrently --kill-others-on-fail \"npm run dev:server\" \"wait-on http://localhost:3000 && npm run dev:client\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "preview": "vite preview",
    "postinstall": "npm run build",
    "test:document-upload": "tsx server/test-document-upload.js"
  },
